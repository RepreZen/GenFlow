		with autoRealizations
			"./LinkSpec_IncludeAutoLinkableToOR.yaml"
		
		objectResource FinancialStatementObject type FinancialStatement
			URI /statementID/{id}
			mediaTypes
				application/json
			method GET getStatementObject
